@media screen and (min-width 2295px)
	body
		font-size: 20px
	// .container
	// 	width 1400px
	header
		padding 15px 0
	.contact
	footer p
		font-size: 16px
	#feedback
		min-height calc(100vh - 755px)
	#main.remind
		min-height calc(100vh - 250px)
@media screen and (max-width 1199px)
	.contact
		font-size: 14px
@media screen and (max-width 991px)
	#logo
		width 180px
		float: none
		display block
		margin 0 auto
	.contacts
		text-align: center
		float: none
		line-height: 20px
	.contact
		display: inline-block
		margin 5px 5px 0
	#feedback
		padding 40px 0
	footer p br
		display: none
@media screen and (max-width 767px)
	#vk
		margin-bottom 25px
@media screen and (max-width 485px)
	#logo
		width 160px
	.contacts
		line-height: 18px
	.contact
		font-size: 12px
		padding-left: 17px
	.contact_skype
		background-size: 12px 12px
	.contact_tel
		background-size: 12px 13px
	.contact_email
		background-size: 16px 12px
		padding-left: 22px
	.pre
		font-size: 20px
	.main_title
		font-size: 36px
		line-height: 1
	.main_text
		font-size: 18px
		line-height: 1.3
	#feedback
		padding 30px 0
	.general_title
		font-size: 26px
		line-height: 1.2
	footer
		padding 15px 0
	footer p
		font-size: 12px
	.soc_buttons a
		margin 10px 5px 
	.main_title
		margin  20px 0
	.remind_main_subtitle
		font-size: 22px
@media screen and (max-width 380px)
	.main_title
		font-size:30px
		line-height: 1
	.general_title
		font-size: 24px
	.remind_main_title
		margin 0 0 20px
		line-height: 1.2
	.remind_main_subtitle
		font-size: 20px